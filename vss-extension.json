{
  "manifestVersion": 1,
  "id": "anchore-task",
  "name": "Anchore Image Scanner",
  "version": "1.0.0",
  "publisher": "santhoshparepu",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Scan images for policy and vulnerabilities",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/anchore128.png"
  },
  "files": [
    {
      "path": "anchoreScanTask"
    }
  ],
  "contributions": [
    {
      "id": "anchore-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "anchoreScanTask"
      }
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "eula.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/sparepu/anchore-vsts-extension.git"
  },
  "public": false
}
